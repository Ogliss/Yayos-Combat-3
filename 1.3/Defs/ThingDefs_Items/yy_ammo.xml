<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="ResourceBase" Name="YCAmmoBase" Abstract="True">
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<stackLimit>250</stackLimit>
		<statBases>
			<MaxHitPoints>20</MaxHitPoints>
			<Mass>0.05</Mass>
			<Flammability>1</Flammability>
			<DeteriorationRate>1.0</DeteriorationRate>
		</statBases>
		<thingCategories>
			<li>yy_ammo_category</li>
		</thingCategories>
		<tradeTags>
			<li>Ammo</li>
		</tradeTags>
		<healthAffectsPrice>false</healthAffectsPrice>
		<soundInteract>Metal_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
	</ThingDef>
	
	<!-- Primitive -->
	<ThingDef ParentName="YCAmmoBase">
		<defName>yy_ammo_primitive</defName>
		<label>primitive ammo</label>
		<description>Ammunition used in primitive and medieval ranged weapons.</description>
		<graphicData>
			<texPath>yy_ammo_primitive</texPath>
		</graphicData>
		<techLevel>Neolithic</techLevel>
	</ThingDef>
	
	<ThingDef ParentName="YCAmmoBase">
		<defName>yy_ammo_primitive_fire</defName>
		<label>primitive explosive ammo</label>
		<description>Ammunition used in primitive and medieval ranged weapons. Used for explosion, flame, and smoke damage type weapons.</description>
		<graphicData>
			<texPath>yy_ammo_primitive_fire</texPath>
		</graphicData>
		<techLevel>Neolithic</techLevel>
	</ThingDef>
	
